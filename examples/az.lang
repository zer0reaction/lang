fn abc_len() {
  'z' - 'a' + 1;
}

fn az(n) {
  let c_current := 'a';
  let c_goal := 'a' + n - 1;
  while c_current <= c_goal {
    putchar(c_current);
    c_current := c_current + 1;
  }

  let i := 1;
  let temp := abc_len();
  while i <= temp - n {
    putchar('-');
    i := i + 1;
  }

  putchar('\n');

  if n > 1 {
    n := n - 1;
    az(n);
  }
}

let temp := abc_len();
az(temp);
